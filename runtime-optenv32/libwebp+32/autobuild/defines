PKGNAME=libwebp+32
PKGSEC=libs
PKGDEP="aosc-aaa+32 libjpeg-turbo+32 libpng+32 libtiff+32 giflib+32 \
        freeglut+32"
BUILDDEP="devel-base+32"
PKGDES="Libraries and support programs to encode and decode images in WebP format (32-bit x86 runtime)"

AUTOTOOLS_AFTER=(
    '--enable-libwebpmux'
    '--enable-libwebpdemux'
    '--enable-libwebpdecoder'
)

ABHOST=optenv32
