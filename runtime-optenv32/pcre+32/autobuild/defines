PKGNAME=pcre+32
PKGSEC=libs
PKGDEP="zlib+32 bzip2+32 readline+32 glibc+32"
BUILDDEP="devel-base+32"
PKGDES="Perl Compatible Regular Expression (32-bit x86 runtime)"

ABHOST=optenv32

AUTOTOOLS_AFTER=(
    '--enable-utf'
    '--enable-unicode-properties'
    '--enable-pcregrep-libz'
    '--enable-pcregrep-libbz2'
    '--enable-pcretest-libreadline'
    '--enable-pcre16'
    '--enable-pcre32'
    '--enable-jit'
)
